<meta charset="utf-8">
<title>{{ title }}</title>
<meta name="description" content="{{ description }}">

<link rel="preload" href="fonts/rubik-v11-latin_cyrillic-regular.woff2" as="font" type="font/woff2" crossorigin>
<link rel="preload" href="fonts/rubik-v11-latin_cyrillic-600.woff2" as="font" type="font/woff2" crossorigin>

<meta http-equiv="x-ua-compatible" content="ie=edge">
<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
<meta name="google" content="notranslate">
<meta name="format-detection" content="telephone=no">

<!--seo start-->
<!--<link rel="canonical" href="http://localhost:3000/">-->
<meta name="robots" content="index, follow">
<!--seo end-->

<!--Open Graph start-->
<meta property="og:locale" content="ru_RU">
<meta property="og:title" content="{{ title }}" />
